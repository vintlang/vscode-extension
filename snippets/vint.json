{
    "Function": {
        "prefix": "func",
        "body": [
            "let ${1:functionName} = func(${2:params}) {",
            "\t${3:// function body}",
            "\treturn ${4:value}",
            "}"
        ],
        "description": "Create a function definition"
    },
    "Constant Declaration": {
        "prefix": "const",
        "body": [
            "const ${1:CONSTANT} = ${2:value}"
        ],
        "description": "Declare a constant"
    },
    "Anonymous Function": {
        "prefix": "afunc",
        "body": [
            "func(${1:params}) {",
            "\t${2:// function body}",
            "\treturn ${3:value}",
            "}"
        ],
        "description": "Create an anonymous function"
    },
    "Async Function": {
        "prefix": "async",
        "body": [
            "async func ${1:functionName}(${2:params}) {",
            "\t${3:// async code}",
            "\treturn ${4:value}",
            "}"
        ],
        "description": "Create an async function"
    },
    "Await Expression": {
        "prefix": "await",
        "body": [
            "let ${1:result} = await ${2:asyncFunction}()"
        ],
        "description": "Await an async function"
