{
  "name": "vint",
  "displayName": "VintLang",
  "description": "The official Vint language support extension",
  "version": "0.1.0",
  "publisher": "tacheraSasi",
  "repository": {
    "type": "git",
    "url": "https://github.com/vintlang/vscode-extension.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vint",
        "aliases": ["Vint", "VintLang"],
        "extensions": [".vint"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vint",
        "scopeName": "source.vint",
        "path": "./syntaxes/vint.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "vint",
        "path": "./snippets/vint.json"
      }
    ]
  }
}
